<block a:if="{{$taroCompReady}}">
    <block a:if="{{type === 'markdown'}}">
        <taro-rich-text-no-wx-parse compid="{{$compid__11}}"></taro-rich-text-no-wx-parse>
    </block>
    <block a:elif="{{type === 'html'}}">
        <view style="{{_$anonymousState__temp}}">
            <wxparser rich-text="{{richText}}"></wxparser>
        </view>
    </block>
</block>