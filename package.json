{
	"name": "taro-v2ex",
	"version": "1.3.0",
	"private": true,
	"description": "多端小程序",
	"templateInfo": {
		"name": "redux",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"v2ex:build:weapp": "cross-env PROJECT_NAME=v2ex TARO_BUILD_TYPE=weapp taro build --type weapp",
		"v2ex:dev:weapp": "cross-env PROJECT_NAME=v2ex npm run dev:weapp -- --watch",
		"v2ex:build:alipay": "cross-env PROJECT_NAME=v2ex TARO_BUILD_TYPE=alipay taro build --type alipay",
		"v2ex:dev:alipay": "cross-env PROJECT_NAME=v2ex npm run build:alipay -- --watch",
		"v2ex:dev:h5": "cross-env PROJECT_NAME=v2ex npm run build:h5 -- --watch",
		"v2ex:build:h5": "cross-env PROJECT_NAME=v2ex TARO_BUILD_TYPE=h5 taro build --type h5",
		"v2ex:test:run": "cross-env PROJECT_NAME=v2ex npm run cypress:open",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"build:weapp": "cross-env TARO_BUILD_TYPE=weapp taro build --type weapp",
		"build:h5": "cross-env TARO_BUILD_TYPE=h5 taro build --type h5",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:rn": "taro build --type rn",
		"commit": "git-cz",
		"release": "standard-version",
		"cypress:open": "cypress open"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"author": "BinZhiZhu",
	"license": "MIT",
	"dependencies": {
		"@tarojs/components": "2.2.6",
		"@tarojs/components-rn": "2.2.6",
		"@tarojs/redux": "2.2.6",
		"@tarojs/redux-h5": "2.2.6",
		"@tarojs/router": "2.2.6",
		"@tarojs/taro": "2.2.6",
		"@tarojs/taro-alipay": "2.2.6",
		"@tarojs/taro-h5": "2.2.6",
		"@tarojs/taro-redux-rn": "2.2.6",
		"@tarojs/taro-rn": "2.2.6",
		"@tarojs/taro-router-rn": "2.2.6",
		"@tarojs/taro-swan": "2.2.6",
		"@tarojs/taro-tt": "2.2.6",
		"@tarojs/taro-weapp": "2.2.6",
		"expo-av": "^6.0.0",
		"expo-brightness": "^6.0.0",
		"expo-image-picker": "^6.0.0",
		"expo-location": "^6.0.0",
		"expo-sensors": "^6.0.0",
		"lodash": "^4.17.21",
		"nerv-devtools": "^1.4.3",
		"nervjs": "^1.4.3",
		"react": "16.3.1",
		"react-native": "^0.60.4",
		"react-native-canvas": "^0.1.32",
		"react-native-image-crop-picker": "^0.23.1",
		"react-native-image-picker": "^0.28.1",
		"react-native-maps": "0.25.0",
		"react-native-unimodules": "^0.5.3",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"standard-version": "^6.0.1",
		"taro-ui": "^2.2.1",
		"taro_rich_text": "^1.0.4",
		"tslib": "^1.8.0"
	},
	"devDependencies": {
		"@tarojs/plugin-babel": "2.2.6",
		"@tarojs/plugin-csso": "2.2.6",
		"@tarojs/plugin-sass": "2.2.6",
		"@tarojs/plugin-uglifyjs": "2.2.6",
		"@tarojs/webpack-runner": "2.2.6",
		"@types/react": "^16.4.8",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/parser": "^1.6.0",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"commitizen": "^3.1.1",
		"cross-env": "^5.2.0",
		"cypress": "^3.4.1",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "^5.16.0",
		"eslint-config-taro": "2.2.6",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-taro": "2.2.6",
		"stylelint": "9.3.0",
		"stylelint-config-taro-rn": "2.2.6",
		"stylelint-taro-rn": "2.2.6",
		"typescript": "^3.0.1",
		"webpack-cli": "^3.3.6"
	}
}
